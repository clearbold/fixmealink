{
    "name": "clearbold/fixmealink",
    "description": "Fix Me a Link is a Craft plugin with link utility functions.",
    "version": "1.0.0",
    "type": "craft-plugin",
    "homepage": "https://github.com/clearbold/fixmealink",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Reeves",
            "homepage": "http://clearbold.com"
        }
